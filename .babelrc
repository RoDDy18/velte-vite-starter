{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-arrow-functions",
    ["@babel/plugin-transform-react-jsx",{
      "pragma":"VelteElement",
      "throwIfNamespace":false
    }],
    "babel-plugin-jsx-control-statements",
    "babel-plugin-loop-optimizer",
    "babel-plugin-macros"
  ]
  
}